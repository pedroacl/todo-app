<!DOCTYPE html>
<html ng-app="myApp">
  <base href="/">
  <head>
    <title>TodoApp</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "layouts/navigation" %>
    <div class="container" ng-controller="MainController as mainController">
      {{ 1 + 2 }}
      <%= yield %>
    </div>
  </body>
</html>
